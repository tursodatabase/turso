---
source: joins.sqltest
expression: |-
  SELECT
          c.customer_id,
          c.name,
          o.order_id,
          o.total_amount
      FROM
          customers c
      INNER JOIN orders o ON c.customer_id = o.customer_id
      WHERE
          o.status = 'completed'
          AND o.total_amount > 100;
info:
  statement_type: SELECT
  tables:
  - c.customer_id
  - customers
  - orders
  setup_blocks:
  - schema
  database: ':memory:'
