test select-modulo-float {
    select 2 % 0.5;
}
expect {
}

test select_positive_infinite_float {
    SELECT 1.7976931348623157E+308 + 1e308;
}
expect {
    Inf
}

test select_negative_infinite_float {
    SELECT -1.7976931348623157E+308 - 1e308;
}
expect {
